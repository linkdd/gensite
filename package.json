{
  "name": "gin",
  "version": "0.3.0",
  "description": "Static website generator",
  "main": "dist/index.js",
  "bin": {
    "gin": "./bin/gin.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "watch": "tsc-watch --onSuccess 'tsc-alias'",
    "ci:format": "prettier --config .prettierrc src/**/*.ts typings/**/*.d.ts --write",
    "ci:lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linkdd/gin.git"
  },
  "keywords": [
    "static",
    "website",
    "generator"
  ],
  "author": "David Delassus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linkdd/gin/issues"
  },
  "homepage": "https://github.com/linkdd/gin#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cmd-ts": "^0.13.0",
    "express": "^4.18.2",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "markdown-it": "^14.0.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-container": "^4.0.0",
    "markdown-it-deflist": "^3.0.0",
    "markdown-it-emoji": "^3.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-ins": "^4.0.0",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-mermaid": "^0.2.0",
    "markdown-it-multimd-table": "^4.2.3",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-wikilinks": "^1.4.0",
    "pegjs": "^0.10.0",
    "grammkit": "^0.7.2",
    "prompt": "^1.3.0",
    "reflect-metadata": "^0.2.1",
    "shiki": "^0.14.7",
    "twing": "^6.0.0-beta.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.6",
    "@types/pegjs": "^0.10.6",
    "@types/prompt": "^1.1.8",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "tsc-alias": "^1.8.8",
    "tsc-watch": "^6.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
